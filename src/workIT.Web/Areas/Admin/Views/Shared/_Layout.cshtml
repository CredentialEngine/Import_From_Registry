@* //This is the Layout file for the main View of the site *@
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <title>Credential Finder | @ViewBag.Title</title>
    <link rel="icon" type="image/png" href="@Url.Content( "~/images/common/favicon.png" )?ver=3" />
    <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script type="text/javascript" src="@Url.Content( "~/Scripts/commonv2.js" )?ver=2"></script>
    <link rel="stylesheet" href="@Url.Content( "~/Styles/Webfonts/Gibson/Gibson.css" )" type="text/css" charset="utf-8" />
    <link rel="stylesheet" href="@Url.Content( "~/Styles/Webfonts/TrumpGothicPro/TrumpGothicPro.css" )" type="text/css" charset="utf-8" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.0/themes/base/jquery-ui.css">
    <link href="@Url.Content( "~/Style/CommonV2" )" rel="stylesheet" type="text/css" />    
    @*<script src="https://use.fontawesome.com/48a8b07d4b.js"></script>*@
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
    @* Not sure if the next 3 lines are necessary *@
    @*<script src="~/Scripts/jquery-ui-1.12.1.js"></script>*@
    <script src="~/Scripts/jquery.timepicker.js"></script>    
    @RenderSection( "css", false )
</head>
<body>

    @Html.Partial( "~/Views/Shared/_Header.cshtml" )
    <div id="content">
        @Html.Partial( "~/Views/Shared/_OldBrowserWarning.cshtml" )
        @RenderBody()
    </div>
    @Html.Partial( "~/Views/Shared/_Footer.cshtml" )

    @Html.Partial( "~/Views/Shared/_ConsoleMessage.cshtml" )
    @Html.Partial( "~/Views/Shared/_ModalWindows.cshtml" )
    @Html.Partial( "~/Views/Shared/_ToolTips.cshtml" )

    @Scripts.Render( "~/bundles/bootstrap" )
    @RenderSection( "scripts", required: false )

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
        ga('create', 'UA-77712853-2', 'auto');
        ga('send', 'pageview');
    </script>

</body>
</html>